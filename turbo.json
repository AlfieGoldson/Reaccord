{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "lib/**"]
    },
    "test": {
      "cache": false,
      "outputs": []
    },
    "test:w": {
      "cache": false,
      "outputs": []
    },
    "build:w": {
      "cache": false,
      "outputs": []
    },
    "check:types": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "docs:build": {
      "cache": false
    },
    "docs:dev": {}
  }
}
